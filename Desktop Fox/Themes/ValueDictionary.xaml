<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:DesktopFox">

    <Color x:Key="Color_Background">#FF2C394A</Color>
    <Color x:Key="Color_Border">DarkGray</Color>
    <Color x:Key="Color_Control">#FF4C77D7</Color>
    <Color x:Key="Color_Text">White</Color>
    <FontFamily x:Key="Default">/Assets/#Nunito</FontFamily>
    <SolidColorBrush x:Key="BackgroundBrush">#FF2C394A</SolidColorBrush>
    <SolidColorBrush x:Key="BorderBrush">DarkGray</SolidColorBrush>
    <Image x:Key="DF_Icon" Source="\MVVM\Views\DF_Icon.ico"/>
    <BooleanToVisibilityConverter x:Key="BoolConverter"/>
    <local:BoolInverter x:Key="BoolInverter"/>
    <local:BoolToIntConverter x:Key="BoolToIntConverter"/>
    <local:BorderClipConverter x:Key="BorderClipConverter"/>
    
</ResourceDictionary>